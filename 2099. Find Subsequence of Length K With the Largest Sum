class Solution {
public:
    vector<int> maxSubsequence(vector<int>& nums, int k) {
        vector<int> sorted = nums;
        sort(sorted.begin(), sorted.end());
        int n = nums.size();
        multiset<int> s;
        vector<int> ans;
        for(int i = n-k; i < n; i++){
            s.insert(sorted[i]);
        }
        for(int i = 0; i < n; i++){
            if(s.contains(nums[i])){
                ans.push_back(nums[i]);
                s.erase(s.find(nums[i]));
            }
        }
        return ans;
    }
};
