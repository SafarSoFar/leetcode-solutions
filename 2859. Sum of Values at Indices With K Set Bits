class Solution {
public:
    int sumIndicesWithKSetBits(vector<int>& nums, int k) {
        int ans = 0;
        for(int i = 0; i < nums.size(); i++){
            int cur = i;
            int setBits = 0;
            while(cur){
                setBits += cur % 2;
                cur /= 2;
            }
            if(setBits == k)
                ans += nums[i];
        }
        return ans;
    }
};
