class Solution {
public:
    int findLucky(vector<int>& arr) {
        unordered_map<int,int> mp;
        int ans = -1;
        for(const auto& val : arr){
            mp[val]++;
        }
        for(const auto& [key,val] : mp){
            if(key == val)
                ans = std::max(ans, key);
        }
        return ans;
    }
};
