class Solution {
public:
    int uniqueMorseRepresentations(vector<string>& words) {
        string morse[26] = {".-","-...","-.-.","-..",".","..-.","--.","....","..",".---","-.-",".-..","--","-.","---",".--.","--.-",".-.","...","-","..-","...-",".--","-..-","-.--","--.."}; //provided
        unordered_set<string> s;
        for(auto& word : words){
            string cur = "";
            for(int i = 0; i < word.size(); i++){
                cur += morse[word[i]-'a'];
            }
            s.insert(cur);
        }
        return s.size();
    }
};
